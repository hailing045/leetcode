class Solution {
    public String longestCommonPrefix(String[] strs) {
        String result = "";
        if(strs.length == 0 || strs == null) return "";
        for(int j = 0; j< strs.length; j++){
          for(int i = 1; i< strs.length; i++){
            if((strs[0]).charAt(j) != (strs[i]).charAt(j) ){
                return result;
                }
            }
        result = result + (strs[0]).charAt(j) ;  
        }
        return result;
    }
}
